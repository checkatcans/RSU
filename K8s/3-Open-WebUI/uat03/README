su - uat03
kubectl get namespace uat03-restricted
kubectl auth can-i create deployments --namespace=uat03-restricted

# Add the Open WebUI Helm repository (if not already added)
helm repo add open-webui https://helm.openwebui.com/
helm repo update

cd /home/uat01/RSU/K8s/3-Open-WebUI/uat01

# Install the chart in the uat01-restricted namespace
helm install open-webui-uat03 open-webui/open-webui \
  --namespace uat03-restricted \
  --values values.yaml
